{
    "name": "gpupo/common-dev-extra",
    "description": "Common PHP tool for require-dev env",
    "homepage": "https://opensource.gpupo.com/common-dev-extra/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["common", "development"],
    "license": "MIT",
    "require": {
        "gpupo/common-dev": "^5.1",
        "phpspec/prophecy": "^1.10",
        "friendsofphp/php-cs-fixer": "^2.16",
        "squizlabs/php_codesniffer": "^3.5",
        "escapestudios/symfony2-coding-standard": "^3.10",
        "phploc/phploc": "^5.0",
        "pdepend/pdepend": "^2.6",
        "phpmd/phpmd": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\CommonDevExtra\\": "src",
            "Gpupo\\CommonDevExtra\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "gpupo/common-sdk": "*"
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
