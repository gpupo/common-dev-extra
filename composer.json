{
    "name": "gpupo/common-dev-extra",
    "description": "Common PHP tool for require-dev env",
    "homepage": "https://opensource.gpupo.com/common-dev-extra/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["common", "development"],
    "license": "MIT",
    "require": {
        "gpupo/common-dev": "*",
        "phpspec/prophecy": "^1.12",
        "friendsofphp/php-cs-fixer": "^3.0 || dev-3.0 || dev-master",
        "squizlabs/php_codesniffer": "^3.5",
        "escapestudios/symfony2-coding-standard": "^3.10",
        "phploc/phploc": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\CommonDevExtra\\": "src",
            "Gpupo\\CommonDevExtra\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "gpupo/common-sdk": "*"
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "phpunit": "vendor/bin/phpunit",
        "test": ["@phpunit"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
